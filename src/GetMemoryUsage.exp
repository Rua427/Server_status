#! /usr/bin/expect


#<host> <ssh user> <ssh pw>


spawn ssh [lindex $argv 1]@[lindex $argv 0] "cat /proc/meminfo"
set timeout 2

expect {
    "yes/no" {
        send "yes\n"
        exp_continue 2
    }
    "*?password:" {
        send "[lindex $argv 2]\n"
    }  
    timeout { exit 0 }
}
interact
